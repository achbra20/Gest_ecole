    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                <h4 class="modal-title" id="myModalLabel">Modifier {{ ceo.matiere.libelle }}</h4>
            </div>
            {{form_errors(formm)}}
            <form role="form"  method="post" action="{{ path('admin_modif_matiere_niveau',{'id':ceo.id}) }}">
                <div class="modal-body">
                    <div class="form-group input-group">
                        <span class="input-group-addon">Mati√®re</span>
                        {{form_widget(formm.matiere,{'attr':{'class':'form-control span12'} })}}
                    </div>
                    <div class="form-group input-group">
                        <span class="input-group-addon">Coefficiant</span>
                        {{form_widget(formm.coefficient,{'attr':{'class':'form-control span12'} })}}
                    </div>
                    <div class="form-group input-group">
                        <span class="input-group-addon">nombre des heure</span>
                        {{form_widget(formm.nbHeure,{'attr':{'class':'form-control span12'} })}}
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-default" data-dismiss="modal">Fermer</button>
                    <button type="Submit" class="btn btn-primary">Modifier</button>
                </div>
                {{ form_rest(formm) }}
            </form>
        </div>
    </div>
